{
    "name":"xendiadyon/contao-full-background-images",
    "description":"Use full background images in Contao Open Source CMS. This extension also provides a full background image slider/fader.",
    "keywords":["contao", "full", "background", "slider", "images"],
    "type":"contao-module",
    "homepage":"http://1up.io",
    "license":"MIT",
    "authors":[
        {
            "name":"David Greminger",
            "email":"dg@1up.io",
            "homepage":"http://1up.io",
            "role":"Developer"
        },
            {
            "name":"Christian Methfessel",
            "email":"dev@xendiadyon.de",
            "homepage":"http://www.christian-methfessel.de",
            "role":"Developer"
        }
    ],
    "support":{
        "email":"hello@1up.io",
        "issues":"https://github.com/1up-lab/contao-full-background-images/issues",
        "irc":"irc://irc.freenode.org/contao.dev",
        "source":"https://github.com/1up-lab/contao-full-background-images"
    },
    "require":{
        "php":">=5.4",
        "contao/core-bundle":"^4.3",
        "contao-community-alliance/composer-installer": "*"
    },
    "replace": {
        "contao-legacy/full-background-images": "self.version",
        "oneup/full-background-images": "self.version"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Oneup\\Contao\\": "src"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/full-background-images"
            }
        },
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
